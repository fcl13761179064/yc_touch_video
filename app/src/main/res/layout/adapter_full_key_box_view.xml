<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cs_key_bg"
    android:layout_width="match_parent"
    android:layout_height="@dimen/fm_dp_30"
    android:layout_marginTop="10dp">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_keybord_letter"
        android:layout_width="@dimen/fm_dp_40"
        android:layout_height="@dimen/fm_dp_30"
        android:background="@drawable/bg_full_search_keybox"
        android:gravity="center"
        android:text="A"
        android:fontFamily="@font/yc_font"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>