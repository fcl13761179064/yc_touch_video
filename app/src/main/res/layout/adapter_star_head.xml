<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
   android:id="@+id/cs_start_head"
    android:layout_width="@dimen/fm_dp_70"
    android:layout_height="wrap_content"
    android:layout_marginRight="25dp"
    >


   <com.google.android.material.imageview.ShapeableImageView
       android:id="@+id/iv_star_img"
       android:layout_width="@dimen/fm_dp_65"
       android:layout_height="@dimen/fm_dp_65"
       android:background="@drawable/fm_shape_default_head_5"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintLeft_toLeftOf="parent"
       app:layout_constraintRight_toRightOf="parent"
       app:shapeAppearanceOverlay="@style/fm_CircleStyle" />


   <androidx.appcompat.widget.AppCompatTextView
       android:id="@+id/at_star_name"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="5dp"
       android:text="周杰伦你"
       android:maxEms="5"
       android:maxLines="1"
       android:ellipsize="end"
       android:textColor="#ffffffff"
       android:textSize="16sp"
       app:layout_constraintTop_toBottomOf="@+id/iv_star_img"
       app:layout_constraintLeft_toLeftOf="@+id/iv_star_img"
       app:layout_constraintRight_toRightOf="@+id/iv_star_img"
       />

</androidx.constraintlayout.widget.ConstraintLayout>