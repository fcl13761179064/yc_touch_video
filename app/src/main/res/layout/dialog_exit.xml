<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/all_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view_back"
        android:layout_width="@dimen/fm_dp_375"
        android:layout_height="@dimen/fm_dp_239"
        android:background="@drawable/fm_shape_all_success_radius_15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="提示"
        android:textColor="@color/white"
        android:textSize="@dimen/fm_text_size_16"
        app:layout_constraintEnd_toEndOf="@id/view_back"
        app:layout_constraintStart_toStartOf="@id/view_back"
        app:layout_constraintTop_toTopOf="@id/view_back" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/fm_dp_25"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@id/view_back"
        app:layout_constraintEnd_toEndOf="@id/view_back"
        app:layout_constraintStart_toStartOf="@id/view_back">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_cancel"
            android:layout_width="@dimen/fm_dp_100"
            android:layout_height="@dimen/fm_dp_33"
            android:layout_marginEnd="35dp"
            android:background="@drawable/fm_ff8157ff_ffe056ff_radis_15"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center"
            android:text="取消"
            android:textColor="@color/white"
            android:textSize="13sp" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/ok_success"
            android:layout_width="@dimen/fm_dp_100"
            android:layout_height="@dimen/fm_dp_33"
            android:background="@drawable/fm_ff8157ff_ffe056ff_radis_15"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center"
            android:text="确定"
            android:textColor="@color/white"
            android:textSize="13sp" />

    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:minHeight="90dp"
        android:text="确定退出欢畅K歌？"
        android:textColor="@color/white"
        android:textSize="@dimen/fm_text_size_13"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/ll"
        app:layout_constraintEnd_toEndOf="@id/view_back"
        app:layout_constraintStart_toStartOf="@id/view_back"
        app:layout_constraintTop_toBottomOf="@id/tv_title" />


</androidx.constraintlayout.widget.ConstraintLayout>